//			String sendfileName = String.format("ebank_pl_%s_%s_snd", batNO, strSeq);
			String sendfileName = String.format("snccb_pz_%s_%s.txt", batNO, strSeq);
			SysPub.appLog("INFO", "STEP2 上传核心文件[%s]", sendfileName);

