						"0", /* 手续费出处 */
						"0", /* 过渡账号 */
						destString[1], /* 入账户名 */
						"0", /* 挂账账号 */
						"0", /* 挂账机构 */
						destString[3], /* 摘要 */
						"0", /* 转出户户名 */
						"0", /* 往来标识 */
						"1", /* 主账号增减标志 */
						"0", /* 对方行号 */
						"0", /* 对方账号 */
						"0", /* 业务类型编号 */
						"0", /* 业务种类 */
						"3", /* 渠道类型 */
						"0", /* 对方账号名称 */
